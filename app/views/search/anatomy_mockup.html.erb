				<% for value in attribute["values"] %>
				<li><span class="term-name" title="<%=h value["id"] %>"><%=h value["name"] %></span></li>
				<% end %>
			</ul>
		</td>
		<td>
			<% if attribute["gene_annotations"]["gene_count"] > 0 %>
				<a href="/search/phenotypes/anatomy/<%=h summary["term"]["id"]%>/genes/<%=h attribute["id"]%>?attribute=true"><%=h attribute["gene_annotations"]["annotation_count"] %> phenotypes</a> (<%=h attribute["gene_annotations"]["gene_count"] %> genes)
