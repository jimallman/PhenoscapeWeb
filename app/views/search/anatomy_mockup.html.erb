	<tr>
		<th><span class="term-name" title="<%=h summary["term"]["id"] %>"><%=h summary["term"]["name"] %></span> Phenotype</th><th>Model Organism Data</th><th>Evolutionary Data</th>
	</tr>
	<% for attribute in summary["attributes"] %>
	<tr>
		<td>
			<span class="term-name" title="<%=h attribute["id"] %>"><%=h attribute["name"] %></span>
